<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <View
        android:layout_width="match_parent"
        android:layout_height="270dp"
        android:background="#66107896"
        android:contentDescription="@null"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/details_details_view20"
        android:layout_width="130dp"
        android:layout_height="200dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="55dp"
        android:background="@color/colorBackground"
        android:contentDescription="@null"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/details_details_view21"
        android:layout_width="50dp"
        android:layout_height="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="55dp"
        android:layout_toEndOf="@+id/details_details_view20"
        android:background="@color/colorBackground"
        app:layout_constraintStart_toEndOf="@+id/details_details_view20"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/details_details_view19"
        android:layout_width="100dp"
        android:layout_height="20dp"
        android:layout_below="@+id/details_details_view21"
        android:layout_gravity="bottom"
        android:layout_marginStart="20dp"
        android:layout_marginTop="2dp"
        android:layout_toEndOf="@+id/details_details_view20"
        android:background="@color/colorBackground"
        app:layout_constraintStart_toEndOf="@+id/details_details_view20"
        app:layout_constraintTop_toBottomOf="@+id/details_details_view21" />

    <View
        android:id="@+id/details_view5"
        android:layout_width="50dp"
        android:layout_height="20dp"
        android:layout_below="@+id/details_details_view19"
        android:layout_marginStart="20dp"
        android:layout_marginTop="8dp"
        android:layout_toEndOf="@+id/details_details_view20"
        android:background="@color/colorBackground"
        app:layout_constraintStart_toEndOf="@+id/details_details_view20"
        app:layout_constraintTop_toBottomOf="@+id/details_details_view19" />

    <View
        android:id="@+id/details_view6"
        android:layout_width="100dp"
        android:layout_height="20dp"
        android:layout_below="@+id/details_view5"
        android:layout_gravity="bottom"
        android:layout_marginStart="20dp"
        android:layout_marginTop="2dp"
        android:layout_toEndOf="@+id/details_details_view20"
        android:background="@color/colorBackground"
        app:layout_constraintStart_toEndOf="@+id/details_details_view20"
        app:layout_constraintTop_toBottomOf="@+id/details_view5" />

    <View
        android:id="@+id/details_view7"
        android:layout_width="50dp"
        android:layout_height="20dp"
        android:layout_below="@+id/details_view6"
        android:layout_marginStart="20dp"
        android:layout_marginTop="8dp"
        android:layout_toEndOf="@+id/details_details_view20"
        android:background="@color/colorBackground"
        app:layout_constraintStart_toEndOf="@+id/details_details_view20"
        app:layout_constraintTop_toBottomOf="@+id/details_view6" />

    <View
        android:id="@+id/details_view8"
        android:layout_width="100dp"
        android:layout_height="20dp"
        android:layout_below="@+id/details_view7"
        android:layout_gravity="bottom"
        android:layout_marginStart="20dp"
        android:layout_marginTop="2dp"
        android:layout_toEndOf="@+id/details_details_view20"
        android:background="@color/colorBackground"
        app:layout_constraintStart_toEndOf="@+id/details_details_view20"
        app:layout_constraintTop_toBottomOf="@+id/details_view7" />

    <View
        android:id="@+id/details_view9"
        android:layout_width="50dp"
        android:layout_height="20dp"
        android:layout_below="@+id/details_view8"
        android:layout_marginStart="20dp"
        android:layout_marginTop="8dp"
        android:layout_toEndOf="@+id/details_details_view20"
        android:background="@color/colorBackground"
        app:layout_constraintStart_toEndOf="@+id/details_details_view20"
        app:layout_constraintTop_toBottomOf="@+id/details_view8" />

    <View
        android:id="@+id/last_item"
        android:layout_width="100dp"
        android:layout_height="20dp"
        android:layout_below="@+id/details_view9"
        android:layout_gravity="bottom"
        android:layout_marginStart="20dp"
        android:layout_marginTop="2dp"
        android:layout_toEndOf="@+id/details_details_view20"
        android:background="@color/colorBackground"
        app:layout_constraintStart_toEndOf="@+id/details_details_view20"
        app:layout_constraintTop_toBottomOf="@+id/details_view9" />

    <View
        android:id="@+id/details_view11"
        android:layout_width="50dp"
        android:layout_height="20dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="30dp"
        android:background="@color/colorBackground"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/last_item" />

    <View
        android:id="@+id/details_view12"
        android:layout_width="0dp"
        android:layout_height="20dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="8dp"
        android:background="@color/colorBackground"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/details_view11" />

    <View
        android:id="@+id/details_view13"
        android:layout_width="0dp"
        android:layout_height="20dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="8dp"
        android:background="@color/colorBackground"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/details_view12" />

    <View
        android:layout_width="0dp"
        android:layout_height="20dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="8dp"
        android:background="@color/colorBackground"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/details_view13" />

</androidx.constraintlayout.widget.ConstraintLayout>
